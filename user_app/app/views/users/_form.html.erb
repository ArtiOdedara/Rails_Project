<%= form_with model: @user do |form| %>
    <div>
      <%= form.label :fname %><br>
      <%= form.text_field :fname %>
      <% @user.errors.full_messages_for(:fname).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
  
    <div>
      <%= form.label :lname%><br>
      <%= form.text_field :lname %>
          <% @user.errors.full_messages_for(:lname).each do |message| %>
        <div><%= message %></div>
      <% end %>
       <%# <% @user.errors.full_messages_for(:lname).each do |message| %> 
    </div>
  
    <div>
      <%= form.label :username%><br>
      <%= form.text_field :username %>
          <% @user.errors.full_messages_for(:username).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
  
    <div>
      <%= form.label :email%><br>
      <%= form.email_field :email%>
          <% @user.errors.full_messages_for(:email).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
  
    <div>
      <%= form.label :password %><br>
      <%= form.password_field :password %>
          <% @user.errors.full_messages_for(:password).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
    <div>
      <%= form.label :Date_Of_Birth %><br>
       <%= form.date_field :Date_Of_Birth %>
           <% @user.errors.full_messages_for(:Date_Of_Birth).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
    <div>
      <%= form.label :phone_number %><br>
      <%= form.text_field :phone_number %>
          <% @user.errors.full_messages_for(:phone_number).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
    <div>
      <%= form.label :term_start_date %><br>
      <%= form.date_field :term_start_date %>
          <% @user.errors.full_messages_for(:term_start_date).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
    
    <div>
      <%= form.label :term_end_date %><br>
      <%= form.date_field :term_end_date %>
          <% @user.errors.full_messages_for(:term_end_date).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
    
    <div>
      <%= form.label :zip_code %><br>
      <%= form.text_field :zip_code %>
          <% @user.errors.full_messages_for(:zip_code).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
    
    <div>
      <%= form.submit %>
    </div>
  <% end %>